
[color]
  status = auto
  branch = auto
  diff = auto

[alias]
  s = status
  d = diff
  ds = diff --staged
  a = add
  co = checkout
  ci = commit
  cim = commit -m
  cia = commit --amend
  b = branch
  f = fetch
  r = reset
  ps = push
  pl = pull
  sub = submodule
  l = log --decorate --graph --date=short --pretty=format:'%C(green)%d %C(cyan)%h %C(magenta)%ad %C(yellow)%an %C(white)%s'
